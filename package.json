{
  "name": "defi-health-service",
  "scripts": {
    "dev": "yarn clean && next dev",
    "build": "next build",
    "start": "next start",
    "prisma:push": "npx prisma db push",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate-dev": "npx prisma migrate dev --name $npm_config_path",
    "prisma:migrate-prod": "npx prisma migrate deploy",
    "prisma:seed": "yarn tsnode prisma/seed.ts",
    "build-start": "yarn clean && next build && next start",
    "tsnode": "yarn node-options && ts-node --esm -p --skip-project -T -r tsconfig-paths/register $npm_config_path",
    "node-options": "NODE_OPTIONS=\"--experimental-fetch --no-warnings --experimental-specifier-resolution=node --trace-deprecation --abort-on-uncaught-exception\"",
    "lint": "next lint",
    "format": "npx prettier --write ./src",
    "clean": "rm -rf .next",
    "clean:full": "rm -rf .next && rm -rf .next-build && rm -rf node_modules .swc"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^4.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^12.2.0",
    "next-auth": "^4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.2.0",
    "react-query": "^3.39.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@swc/core": "^1.2.207",
    "@types/eslint": "^8.4.3",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "clsx": "^1.1.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-status-typed": "^1.0.1",
    "husky": "^8.0.1",
    "localtunnel": "^2.0.2",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "windicss-webpack-plugin": "^1.7.3",
    "zod-prisma": "^0.5.4"
  },
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ComposableFi/defi-health-service.git"
  }
}
